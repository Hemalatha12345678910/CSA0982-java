public class StarPattern {
    public static void main(String[] args) {
        int numberOfRows = 5;
        printStarPattern(numberOfRows);
    }
    public static void printStarPattern(int numberOfRows) {
        for (int i = 1; i <= numberOfRows; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print("* ");
          }
            System.out.println();
        }
    }
}
