class Overload {
    private int value1;
    private int value2;
    // Method to assign a single value to both value1 and value2
    public void assignValues(int val) {
        value1 = val;
        value2 = val;
    }

    // Method to assign different values to value1 and value2
    public void assignValues(int val1, int val2) {
        value1 = val1;
        value2 = val2;
    }

    public void printValues() {
        System.out.println("Value 1: " + value1);
        System.out.println("Value 2: " + value2);
    }

    public static void main(String[] args) {
        Overload overload = new Overload();

        // Assign a single value to both variables
        overload.assignValues(5);
        System.out.println("After assigning a single value:");
        overload.printValues();

        // Assign different values to each variable
        overload.assignValues(10, 20);
        System.out.println("After assigning different values:");
        overload.printValues();
    }
}
